// This sample shows the structure of the Firebase DB.
{
	"hives": {
		"001" : { // device id, can have multiple hives.
			"name" : "[name here]", // optional, if giving each hive a nickname
			"values" : [ // making this a subset, as there will be lots of datetimes. Access will be easier.
				{
					"dateTime" : // will have 1 of these for every minute of the day -> 1440
						{
							"date" : date,
							"temperature" : {
								"inside" : int,
								"outside" : int,
								"base" : int
							},
							"humidity" : {
								"inside" : int,
								"outside" : int,
								"base" : int
							},
							"pressure" : int,
							"co2" : int
						}
				},
			],
			// checking whethere there is ice or snow
			"iceStatus" : bool,
			// checking status of different mechanisms
			"heaterStatus" : bool,
			"flapperStatus" : bool,
			"fanStatus" : bool
			"testLed1Status" : bool,
			"testLed2Status" : bool
		}
	}
}